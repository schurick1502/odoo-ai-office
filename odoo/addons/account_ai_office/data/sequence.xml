<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_ai_case" model="ir.sequence">
            <field name="name">AI Case</field>
            <field name="code">account.ai.case</field>
            <field name="prefix">AIC-%(year)s-</field>
            <field name="padding">5</field>
        </record>

        <record id="config_service_url" model="ir.config_parameter">
            <field name="key">ai_office.service_url</field>
            <field name="value">http://ai_office_service:8100</field>
        </record>
    </data>
</odoo>
